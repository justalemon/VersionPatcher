    "main": "lib/index.js",
    "scripts": {
        "build": "tsc && ncc build index.ts --out dist --minify",
        "lint": "eslint .",
        "test": "jest"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/justalemon/VersionPatcher.git"
    },
    "author": "Hannele \"Lemon\" Ruiz",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/justalemon/VersionPatcher/issues"
    },
    "homepage": "https://github.com/justalemon/VersionPatcher#readme",
    "dependencies": {
        "@actions/core": "^1.10.0",
        "@actions/github": "^5.1.1",
        "@actions/glob": "^0.3.0",
        "xml2js": "^0.4.23"
    },
    "devDependencies": {
        "@octokit/webhooks-definitions": "^3.67.3",
        "@types/jest": "^28.1.3",
        "@types/xml2js": "^0.4.11",
        "@typescript-eslint/eslint-plugin": "^5.45.0",
        "@typescript-eslint/parser": "^5.45.0",
        "@vercel/ncc": "^0.34.0",
        "eslint": "^8.29.0",
        "eslint-plugin-jest": "^27.1.6",
        "jest": "^28.1.1",
        "ts-jest": "^28.0.5",
        "typescript": "^4.8.4"
    }
